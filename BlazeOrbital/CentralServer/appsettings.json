{
  "Apps": {
    "ManufacturingHub": { "Origin": "https://cherryclient.azurewebsites.net" },
    "MissionControl": { "Origin": "https://localhost:44423" }
  },
  "ConnectionStrings": {
    "DefaultConnection": "DataSource=app.db"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFramework": "Warning"
    }
  },
  "IdentityServer": {
    "Key": {
      "Type": "Development"
    },
    "Clients": {
      "Cherry.ManufacturingHub": {
        "Profile": "SPA",
        "RedirectUri": "https://cherryclient.azurewebsites.net/authentication/login-callback",
        "LogoutUri": "https://cherryclient.azurewebsites.net/authentication/logged-out"
      },
      "Cherry.MissionControl": {
        "Profile": "SPA",
        "RedirectUri": "https://localhost:44423/authentication/login-callback",
        "LogoutUri": "https://localhost:44423/authentication/logged-out"
      },
      "Cherry.Accounting": {
        "Profile": "IdentityServerSPA"
      }
    }
  },
  "AllowedHosts": "*"
}
